(defroutine
"Key Press (Left Hand)"
#(:RGB 0.7 0.0 0.0)
(deftask
1
"Initiate Key Press"
"Cognitive Operator"
"Gaussian CV"
'("50" "(random 0.1 1.0 1)")
0 100
nil '(2))
(deftask
2
"Home to"
"Left Hand Operator"
"Gamma CV"
'("200" "(random 0.1 1.0 1)")
200 200
'(1) '(3)
:measurable t)
(deftask
3
"Key Down"
"Left Hand Operator"
"Gamma CV"
'("70" "(random 0.1 1.0 1)")
400 200
'(2) '(4 5)
:measurable t)
(deftask
4
"Key Up"
"Left Hand Operator"
"Gamma CV"
'("0" "(random 0.1 1.0 1)")
600 200
'(3) nil
:measurable t)
(deftask
5
"System Response"
"System Resource"
"Gaussian CV"
'("0" "(random 0.1 1.0 1)")
600 0
'(3) nil
:measurable t)
)
