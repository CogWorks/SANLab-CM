(defroutine
"Prepped Mouse Click"
#(:RGB 0.7 0.3 0.0)
(deftask
1
"Initiate Click"
"Cognitive Operator"
"Gamma CV"
'("50" "(random 0.1 1.0 1)")
0 100
nil '(2))
(deftask
2
"init - click"
"Right Hand Operator"
"Gamma CV"
'("0" "(random 0.1 1.0 1)")
100 200
'(1) '(3)
:measurable t)
(deftask
3
"click"
"Right Hand Operator"
"Gamma CV"
'("125" "(random 0.1 1.0 1)")
300 200
'(2) '(4))
(deftask
4
"clicked"
"System Resource"
"Gamma CV"
'("0" "(random 0.1 1.0 1)")
400 100
'(3) nil)
)
