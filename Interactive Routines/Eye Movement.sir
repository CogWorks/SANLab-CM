(defroutine
"Eye Movement"
#(:RGB 0.0 0.7 0.0)
(deftask
1
"Initiate Eye Movement"
"Cognitive Operator"
"Gamma CV"
'("50" "(random 0.1 1.0 1)")
0 100
nil '(2))
(deftask
5
"System Display"
"System Resource"
"Gamma CV"
'("0" "(random 0.1 1.0 1)")
200 0
nil '(3)
:measurable t)
(deftask
2
"Eye Movement"
"Eye Movement Operator"
"Gamma CV"
'("30" "(random 0.1 1.0 1)")
200 200
'(1) '(3)
:measurable t)
(deftask
3
"Perceive"
"Perceptual Operator (Visual)"
"Gamma CV"
'("290" "(random 0.1 1.0 1)")
400 0
'(2 5) '(4))
(deftask
4
"Verify"
"Cognitive Operator"
"Gamma CV"
'("50" "(random 0.1 1.0 1)")
600 100
'(3) nil)
)
