(defroutine
"Cognitive Analysis"
#(:RGB 1.0 1.0 0.0)
(deftask
1
"initiate"
"Cognitive Operator"
"Gamma CV"
'("50" "(random 0.1 1.0 1)")
0 0
nil '(2))
(deftask
2
"attend"
"Cognitive Operator"
"Gamma CV"
'("50" "(random 0.1 1.0 1)")
200 0
'(1) '(3))
(deftask
3
"verify"
"Cognitive Operator"
"Gamma CV"
'("50" "(random 0.1 1.0 1)")
400 0
'(2) nil)
)