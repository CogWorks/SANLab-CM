(defroutine
"Fixation-old"
#(:RGB 0.0 0.7 0.0)
(deftask
1
"Initiate Eye Movement"
"Cognitive Operator"
"Gamma CV"
'("118" "(random 0.1 1.0 1)")
0 300
nil '(2))
(deftask
5
"System Display"
"System Resource"
"Gamma CV"
'("0" "(random 0.1 1.0 1)")
100 0
nil '(3)
:measurable t)
(deftask
2
"Eye Movement"
"Eye Movement Operator"
"Gamma CV"
'("30" "(random 0.1 1.0 1)")
100 400
'(1) '(3 6)
:measurable t)
(deftask
3
"Perceive"
"Perceptual Operator (Visual)"
"Gamma CV"
'("178" "(random 0.1 1.0 1)")
200 200
'(2 5) '(4))
(deftask
4
"Verify"
"Cognitive Operator"
"Gamma CV"
'("118" "(random 0.1 1.0 1)")
300 300
'(3) nil)
(deftask
6
"Fixate"
"Fixation Operator"
"Gamma CV"
'("100" "(random 0.1 1.0 1)")
300 400
'(2) nil
:measurable t)
)
